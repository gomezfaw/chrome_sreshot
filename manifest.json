{
  "manifest_version": 3,
  "name": "SREshot",
  "description": "Simple Extension to make screenshots",
  "version": "0.0.1",
  "offline_enabled": true,
  "key": "",
  "background": {
    "service_worker": "worker.js"
  },
  "icons": {
    "128": "data/icons/128x128.png"
  },
  "options_ui": {
    "page": "data/options/index.html"
  },
  "action": {
    "default_popup": "data/popup/index.html"
  },
  "commands": {
    "_execute_action": {
      "description": "Press the action button"
    },
    "captureArea": {
      "description": "Capture area",
      "suggested_key": "Alt+S"
    }
  },
  "oauth2": {
    "client_id": "936027930471-nqj7dro8c6efgstt53njr4smum36hiau.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive.appdata",
      "https://www.googleapis.com/auth/drive.file"
    ]
  },
  "permissions": [
    "contextMenus",
    "activeTab",
    "scripting",
    "downloads",
    "notifications",
    "storage",
    "clipboardWrite",
    "identity"
  ]
}
